#include <iostream>
 #include <cstdlib>
 using namespacestd;
 // Global variable int
 b =60;
 // Namespacedeclarations
 namespace N1
 {
 }
 int a = 40;
 namespace N2
 {
 }
 int a = 2;
 // Function to demonstrate local, global, and namespace scope
 void fun()
 {
 int a = 2; // Local variable inside fun
  cout << "FunA: " << a << " " << ::b << endl; // local a, global b
 }
 int main()
 {
 system(“color F0”);
